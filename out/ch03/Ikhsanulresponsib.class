package ch03;
import java.util.Scanner;
public class Ikhsanulresponsib {
    public static void main(String[] args) {
        Scanner ras = new Scanner (System.in);
        int x;
        int y;
        int z;
        System.out.println("Masukkan nilai x");
        x = ras.nextInt();
        System.out.println("Masukan nilai y");
        y = ras.nextInt();
        System.out.println("Masukkan nilai z");
        z =  ras.nextInt();
        if (x > y && x > z){
            System.out.println("Nilai tertinggi adalah :" + x);
        }
        else if (y > x && y > z){
        System.out.println("Nilai tertinggi adalah :" + y);
        }
        else {
            System.out.println("Nilai tertinggi adalah :" + z);
        }
    }
    