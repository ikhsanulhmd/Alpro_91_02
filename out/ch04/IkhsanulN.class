package ch04;
public class IkhsanulN {
        public static void main(String[] args) {
        int[] angka = {82,12,41,38,19,26,9,48,20,55,8,32,3};
        for(int x = 0; x < angka.length; x++){
            if (x != 12){
                int hasil = angka[x] + angka[x+1];
                System.out.println(angka[x] +"+"+ angka[x+1] + " = " +hasil);
            }
        }
    }
}